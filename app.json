{
  "expo": {
    "name": "ZenRoomFrontendMobile",
    "slug": "ZenRoomFrontendMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/start.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "API_URL": "http://192.168.2.138:8080/api/v1",
      "WS_BASE": "ws://192.168.2.138:8080"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}